   <Steppable Type="DiffusionSolverFE">
      
      <!-- Specification of PDE solvers -->
      <DiffusionField Name="MigC">
         <DiffusionData>
            <FieldName>MigC</FieldName>
            <GlobalDiffusionConstant>14.28</GlobalDiffusionConstant>
            <GlobalDecayConstant>0.001428</GlobalDecayConstant>
            <DiffusionCoefficient CellType="Follower">14.28</DiffusionCoefficient>
            <DiffusionCoefficient CellType="Leader">14.28</DiffusionCoefficient>
            <DiffusionCoefficient CellType="Source">14.28</DiffusionCoefficient>
            <DecayCoefficient CellType="Follower">0.001428</DecayCoefficient>
            <DecayCoefficient CellType="Leader">0.001428</DecayCoefficient>
            <DecayCoefficient CellType="Source">0.001428</DecayCoefficient>
         </DiffusionData>
         <SecretionData>
            <!-- When secretion is defined inside DissufionSolverFEall secretion constants are saled automaticly to account for extra calls of the solver when handling large diffusion constants -->
            <Secretion Type="Source">12.997 </Secretion> <!-- PARAM Change Leader secretion -->
         </SecretionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantDerivative PlanePosition="Min" Value="0"/>
               <ConstantDerivative PlanePosition="Max" Value="0"/>
            </Plane>
            <Plane Axis="Y">
               <ConstantDerivative PlanePosition="Min" Value="0"/>
               <ConstantDerivative PlanePosition="Max" Value="0"/>
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
      
      <DiffusionField Name="LC">
        <DiffusionData>
           <FieldName>LC</FieldName>
           <GlobalDiffusionConstant>0.8</GlobalDiffusionConstant>
           <GlobalDecayConstant>0.08</GlobalDecayConstant>
           <!-- Additional options are: -->
           <!-- <InitialConcentrationExpression>x*y</InitialConcentrationExpression> -->
           <!-- <ConcentrationFileName>INITIAL CONCENTRATION FIELD - typically a file with path Simulation/NAME_OF_THE_FILE.txt</ConcentrationFileName> -->
           <DiffusionCoefficient CellType="Leader">0.8</DiffusionCoefficient>
           <DiffusionCoefficient CellType="Follower">0.8</DiffusionCoefficient>
           <DiffusionCoefficient CellType="Source">0.8</DiffusionCoefficient>
           <DecayCoefficient CellType="Leader">0.08</DecayCoefficient>
           <DecayCoefficient CellType="Follower">0.08</DecayCoefficient>
           <DecayCoefficient CellType="Source">0.08</DecayCoefficient>
        </DiffusionData>
        <SecretionData>
           <Secretion Type="Leader">0.034</Secretion>
           <Secretion Type="Follower">0.014</Secretion>
           <Secretion Type="Intermediate">0.024</Secretion>
        </SecretionData>
        <BoundaryConditions>
           <Plane Axis="X">
               <ConstantDerivative PlanePosition="Min" Value="0"/>
               <ConstantDerivative PlanePosition="Max" Value="0"/>
           </Plane>
           <Plane Axis="Y">
               <ConstantDerivative PlanePosition="Min" Value="0"/>
               <ConstantDerivative PlanePosition="Max" Value="0"/>
           </Plane>
        </BoundaryConditions>
      </DiffusionField>
      
      
     <ChemicalField Name="ATTR">  
            <!--     <ChemotaxisByType Type="Macrophage" Lambda="20"/>   	  -->
        </ChemicalField>
      
      
      <DiffusionField>
            <DiffusionData>
                <FieldName>ATTR</FieldName>
                <DiffusionConstant>0.10</DiffusionConstant>
                <DecayConstant>0.000</DecayConstant>
                <DiffusionCoefficient CellType="Wall">1.0</DiffusionCoefficient>
            </DiffusionData>
            <SecretionData>
                <Secretion Type="Wall">.01</Secretion>
            </SecretionData>
      </DiffusionField>
      
