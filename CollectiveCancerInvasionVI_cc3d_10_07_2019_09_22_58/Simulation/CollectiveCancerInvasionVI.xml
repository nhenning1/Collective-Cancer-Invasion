<CompuCell3D Revision="20190906" Version="4.1.0">
   
   <Metadata>
   
      <!-- Basic properties simulation -->
      <NumberOfProcessors>1</NumberOfProcessors>
      <DebugOutputFrequency>10</DebugOutputFrequency>
      <!-- <NonParallelModule Name="Potts"/> -->
      
   </Metadata>
   
   <Potts>
      
      <!-- Basic properties of CPM (GGH) algorithm -->
      <Dimensions x="100" y="100" z="1"/>
      <Steps>2000</Steps> 
      <Temperature>10.0</Temperature>
      <NeighborOrder>1</NeighborOrder>
      
      <!--Uncomment the following two lines to allow for periodic boundary conditions-->
      <!-- <Boundary_x>Periodic</Boundary_x> -->
      <!--<Boundary_y>Periodic</Boundary_y> -->
      
   </Potts>
   
   <Plugin Name="CellType">
      
      <!-- Listing all cell types in the simulation -->
      <CellType Freeze="" TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="Leader"/>
      <CellType TypeId="2" TypeName="Follower"/>
      <CellType TypeId="3" TypeName="Intermediate"/>
      <CellType Freeze="" TypeId="4" TypeName="Source"/>
      <CellType Freeze="" TypeId="5" TypeName="Wall"/> <!--VegF from enviornment -->
   </Plugin>
   
   <Plugin Name="VolumeLocalFlex">
   
   </Plugin>
   
   <Plugin Name="ExternalPotential">
      
      <!-- External force applied to cell. Each cell type has different force. -->
      <!-- For more flexible specification of the constraint (done in Python) please use ExternalPotentialLocalFlex plugin -->
      <!-- Algorithm options are: PixelBased, CenterOfMassBased -->
      <Algorithm>PixelBased</Algorithm>
      <ExternalPotentialParameters CellType="Leader" x="0" y="0" z="0.0"/>
      <ExternalPotentialParameters CellType="Follower" x="0" y="0" z="0.0"/>
      <ExternalPotentialParameters CellType="Intermediate" x="0" y="0" z="0.0"/>
   </Plugin>
   
   <Plugin Name="CenterOfMass">
      <!-- Module tracking center of mass of each cell -->
   </Plugin>
   
   <Plugin Name="Contact">
      <!-- Specification of adhesion energies -->
      <Energy Type1="Medium" Type2="Medium">10.0</Energy>
      <Energy Type1="Medium" Type2="Leader">14.0</Energy>
      <Energy Type1="Medium" Type2="Follower">10.0</Energy>
      <Energy Type1="Medium" Type2="Intermediate">13.0</Energy>
      <Energy Type1="Leader" Type2="Leader">100.0</Energy>
      <Energy Type1="Leader" Type2="Follower">3.0</Energy>
      <Energy Type1="Leader" Type2="Intermediate">2.0</Energy>
      <Energy Type1="Follower" Type2="Follower">1.0</Energy>
      <Energy Type1="Follower" Type2="Intermediate">5.0</Energy>
      <Energy Type1="Intermediate" Type2="Intermediate">9.0</Energy>
      <NeighborOrder>1</NeighborOrder>
   </Plugin>
   
   <Plugin Name="Chemotaxis">
      
      <!-- You may repeat ChemicalField element for each chemical field declared in the PDE solvers -->
      <!-- Specification of chemotaxis properties of select cell types. -->
      <ChemicalField Name="MigC" Source="DiffusionSolverFE">
         <ChemotaxisByType Lambda="-5000" Type="Leader"/> <!-- PARAM Change chemotaxis strength -->
      </ChemicalField>
   </Plugin>
   
   <Plugin Name="LengthConstraint">
      <LengthEnergyParameters CellType="Follower" LambdaLength="2.0" TargetLength="25"/>
      <LengthEnergyParameters CellType="Leader" LambdaLength="2.0" TargetLength="10"/>
      <LengthEnergyParameters CellType="Source" LambdaLength="2.0" TargetLength="25"/>
   </Plugin>
   
   
   <Plugin Name="ConnectivityGlobal">
        <FastAlgorithm/>
    </Plugin>
  
  
